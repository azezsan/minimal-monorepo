    "check": "tsc --noEmit"
  },
  "dependencies": {
    "@acme/db": "workspace:*",
    "@elysiajs/swagger": "^1.2.0",
    "elysia": "^1.2.2"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@cloudflare/workers-types": "catalog:",
    "typescript": "catalog:",
    "wrangler": "catalog:"
  },
  "module": "src/index.js"
}

{
  "name": "@acme/api",
  "version": "1.0.50",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./types": "./src/worker-configuration.d.ts" 
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "wrangler dev --persist-to ../../",

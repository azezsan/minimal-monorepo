{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "inputs": [
                "$TURBO_DEFAULT$",
                ".env*"
            ],
            "outputs": [
                ".svelte-kit/**",
                ".vercel/**",
                ".cache/**",
                "dist/**"
            ]
        },
        "lint": {},
        "format": {},
        "deploy": {
            "dependsOn": [
                "^build"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        }
    }
}